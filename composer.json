{
    "name": "sergeil/expander-bundle",
    "description": "Provides an easy way how to leverage native DIC tags based mechanism for creating extension-points.",
    "keywords": ["extensibility"],
    "homepage": "https://github.com/sergeil/ExpanderBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@gmail.com",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phake/phake": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Sli\\ExpanderBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
